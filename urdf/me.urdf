<xml version="1.0"/>
<?xml version="1.0"?>
<robot name="me">
    <link name="map"/>
    <joint name="fixed" type="fixed">
        <parent link="map"/>
        <child link="chassis"/>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    </joint>
    <joint name="rota" type="continuous">
        <parent link="chassis"/>
        <child link="leg"/>
        <axis xyz="0 0 1"/>
        <origin xyz="5 5 0" rpy="1.57 0 0"/>
    </joint>
    <joint name="knee" type="revolute">
        <parent link="leg"/>
        <child link="ankle"/>
        <axis xyz="0 0 1"/>
        <limit lower="-3.14159" upper="3.14159" velocity="50.0" effort="1000.0"/>
        <origin xyz="0 -4.5 0" rpy="0 0 0"/>
    </joint>
    
    <link name="chassis">
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <box size="10.0 10.0 0.5"/>
            </geometry>
        </collision>
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <box size="10.0 10.0 0.5"/>
            </geometry>
        </visual>
        <inertial>
            <mass value="10"/>
            <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
        </inertial>
    </link>
    <link name="leg">
        <collision>
            <origin xyz="0.0 -2.5 0" rpy="-1.57 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.25" length="5"/>
            </geometry>
        </collision>
        <visual>
            <origin xyz="0.0 -2.5 0" rpy="-1.57 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.25" length="5"/>
            </geometry>
        </visual>
        <inertial>
            <mass value="10"/>
            <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
        </inertial>
    </link>
    <link name="ankle">
        <collision>
            <origin xyz="0.0 -2.5 0.0" rpy="-1.57 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.25" length="5"/>
            </geometry>
        </collision>
        <visual>
            <origin xyz="0.0 -2.5 0.0" rpy="-1.57 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.25" length="5"/>
            </geometry>
        </visual>
        <visual>
            <origin xyz="0 0 0.0" rpy="0.0 0.0 0.0"/>
            
            <geometry>
                <sphere radius="0.5"/>
            </geometry>
        </visual>
        <inertial>
            <mass value="10"/>
            <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
        </inertial>
    </link>
</robot>